@using ProyectoDB2.DTOs
@model List<TipoProductoDTO>

@{
    ViewData["Title"] = "Index";
}

<h1>Tipos de Producto</h1>

@* Lista de productos *@
<table class="table table-bordered table-striped">
    <tr>
        <th>Id</th>
        <th>Nombre</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Nombre</td>
        </tr>
    }
</table>
