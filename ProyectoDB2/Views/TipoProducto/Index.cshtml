@{
    ViewData["Title"] = "Index";
}

@* TipoProductoDTO *@


<h1>Index</h1>
<p>Ejemplo</p>
<p>Otro ejemplo</p>


@* Lista de productos *@
<table class="table table-bordered table-striped">
    <tr>
        <th>Id</th>
        <th>Nombre</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Nombre</td>
        </tr>
    }
</table>
